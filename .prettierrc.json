{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "trailingComma": "all",
  "printWidth": 100,
  "importOrder": [
    "^(fs|path|os|crypto|util|stream|url|zlib|http|https)(/.*)?$",
    "^[a-zA-Z]",
    "^@src/(.*)$",
    "^@tests/(.*)$",
    "^\\./.*"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
